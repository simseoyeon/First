{{>layouts/header}}
<table class="table">
    <thead>
    <tr>
        <th>Id</th>
        <th>Email</th>
        <th>Password</th>
    </tr>
    </thead>
    <tbody>
    {{#member}}
        <tr>
            <th>{{id}}</th>
            <td>{{email}}</td>
            <td>{{password}}</td>
        </tr>
    {{/member}}
    </tbody>
</table>
<a href="/members/{{member.id}}/edit" class="btn btn-primary">수정하기</a>
<a href="/members/{{member.id}}/delete" class="btn btn-primary">삭제하기</a>
<a href="/members">Go to memberList</a>
{{>layouts/footer}}